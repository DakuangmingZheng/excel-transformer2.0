+
+import {formatPostsPerDayData, formatUsersWithPostsPerDayData} from 'components/analytics/format';
+
+import MeetingListCount from '../meetings_list_count.jsx';
+import MeetingListTable from '../meetings_list_table.jsx';
+import MeetingListSearch from '../meetings_list_search.jsx';
+import {getSiteURL} from 'utils/url';
+
+const siteURL = getSiteURL();
+
+export default class MeetingList extends React.PureComponent {
+    static propTypes = {
+    }
+
+    constructor(props) {
+        super(props);
+
+        this.state = {
+            sortName: '',
+            recentlyActiveUsers: [],
+            newUsers: [],
+            rows: [],
+            countData: {
+                'meetingCount': 0,
+                'nowMeetingCount': 0,
+                'meetingUserCount': 0,
+                'nowMeetingUserCount': 0,
+                'meetingTimes': 0,
+                'nowMeetingTimes': 0,
+                'repeatMeetingCount': 0
+            },
+            loadingDatas: true,
+            errorMsg: '',
+        };
+    }
+    componentWillMount() {
+        const referUrl = sessionStorage.getItem("refer_url")
+        const startDate = sessionStorage.getItem("start_date")
+        const endDate = sessionStorage.getItem("end_date")
+        const meetingName = sessionStorage.getItem("meeting_name")
+        const sortName = sessionStorage.getItem("sort_name")
+        if (referUrl == 'detail') {
+            this.changeDatas(startDate, endDate, meetingName, sortName)
+        }
+    }
+
+    getData = async (data) => {
+        // url作成
+        this.setState({loadingDatas: true});
+        var pathname = "/api/v4/meeting_mng"
+
+        var resCountData = {
+            'meetingCount': 0,
+            'nowMeetingCount': 0,
+            'meetingUserCount': 0,
+            'nowMeetingUserCount': 0,
+            'meetingTimes': 0,
+            'nowMeetingTimes': 0,
+            'repeatMeetingCount': 0
+        }
+        // 会議作成
+        var url = siteURL+pathname;
+
+        await axios({
+            url,
+            data:data,
+            method:'post',
+            headers:{
+                'Content-Type':'application/json',
+                'X-Requested-With':'XMLHttpRequest'
+            }
+        }).then(res => {
+            if (res.data.status && res.data.status =="NG") {
+                this.setState({
+                    rows: [], 
+                    countData: resCountData, 
+                    loadingDatas: false,
+                    errorMsg: res.data.message
+                });
+            } else {
+                if (!res.data.meetings_data) {
+                    this.setState({
+                        rows: [], 
+                        countData: resCountData, 
+                        loadingDatas: false,
+                        errorMsg: res.data.message
+                    });
+                    return 
+                }
+                const stabilized = res.data.meetings_data.map((el, index) => {
+                    if (!el.is_repeat) {
+                        resCountData.meetingCount += parseInt(el.meeting_count, 10)
+                        resCountData.nowMeetingCount += parseInt(el.meet_meeting_count, 10)
+                        
+                        resCountData.meetingUserCount += parseInt(el.user_count, 10)
+                        resCountData.nowMeetingUserCount += el.meet_user_count == '-' ? 0 :  parseInt(el.meet_user_count, 10)
+    
+                        resCountData.meetingTimes += parseInt(el.meeting_times, 10)
+                        resCountData.nowMeetingTimes += parseInt(el.meet_meeting_times, 10)
+                    } else {
+                        resCountData.repeatMeetingCount += parseInt(el.meeting_count, 10)
+                    }
+
+                    return [el, index]
+                });
+                stabilized.sort((a, b) => {
+                    var order = 0
+                    if (b[0][data.sort_name] < a[0][data.sort_name]) {
+                        order = 1;
+                    }
+                    if (b[0][data.sort_name] > a[0][data.sort_name]) {
+                        order =  -1;
+                    }
+                    if (order !== 0) return order;
+                    return a[1] - b[1];
+                });
+                const resdata = stabilized.map((el) => el[0])
+                this.setState({rows: resdata, countData: resCountData, loadingDatas: false, errorMsg: ""});
+            }
+        })
+    }
+    changeDatas = (startDate, endDate, meetingName, sortName) => {
+        // 予約パラメータ作成
+        this.setState({sortName: sortName});
+        var data = {
+            "start_date":startDate,
+            "end_date":endDate,
+            "meeting_name":meetingName,
+            "sort_name":sortName
+        }
+
+        sessionStorage.setItem("start_date", startDate)
+        sessionStorage.setItem("end_date", endDate)
+        sessionStorage.setItem("meeting_name", meetingName)
+        sessionStorage.setItem("sort_name", sortName)
+        this.getData(data);
+    }
+    backError = (errorMsg) => {
+        this.setState({
+            errorMsg: errorMsg
+        });
+    }
+
+    render() {
+        let meeting_list = (
+            <div></div>
+        );
+        if (this.state.sortName != '') {
+            meeting_list = ( 
+                <MeetingListTable 
+                    datas={this.state.rows} 
+                    countData={this.state.countData} 
+                    sort={this.state.sortName} 
+                    changeDatas = {this.changeDatas} 
+                    backError = {this.backError} 
+                    loadingDatas = {this.state.loadingDatas}
+                    />
+            );
+        }
+        let count_list = (
+            <div></div>
+        );
+        if (this.state.sortName != '') {
+            count_list = ( 
+                <MeetingListCount countData={this.state.countData}/>
+            );
+        }
+        return (
+            <div className='wrapper--fixed team_statistics'>
+                <div className='admin-console__header team-statistics__header-row'>
+                    <div className='team-statistics__header'>
+                        <FormattedMarkdownMessage
+                            id='admin.meeting_list.title'
+                            defaultMessage='Meeting List'
+                        />
+                    </div>
+                </div>
+
+                <div className='admin-console__wrapper'>
+                    <div className='admin-console__content'>
+                        <MeetingListSearch changeDatas = {this.changeDatas} errorMsg = {this.state.errorMsg}/>
+                        {count_list}
+                        {meeting_list}
+                    </div>
+                </div>
+            </div>
+        );
+    }
+}
