+                    var wekdaydif = Math.abs(meeting[i] - meeting[ii]);
+                    if(wekdaydif < weekdaydifference){
+                        weekdaydifference = wekdaydif;
+                    }else if((7-wekdaydif) < weekdaydifference){
+                        weekdaydifference = 7-wekdaydif;
+                    }
+                }
+            }
